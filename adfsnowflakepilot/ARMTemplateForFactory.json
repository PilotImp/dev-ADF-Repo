						"     deletable:false,",
						"     insertable:true,",
						"     updateable:false,",
						"     upsertable:false,",
						"     format: 'table',",
						"     stageInsert: true,",
						"     mapColumn(",
						"          VIN = {VIN (1-10)},",
						"          COUNTY = County,",
						"          CITY = City,",
						"          STATE = State,",
						"          POSTAL_CODE = {Postal Code},",
